<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"

    android:layout_height="match_parent"
    android:background="#011127"
    tools:context=".MainCultura">

    <ImageView
        android:id="@+id/img_Ant8"
        android:layout_width="63dp"
        android:layout_height="33dp"
        app:srcCompat="@drawable/flecha"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="464dp" />

    <TextView
        android:id="@+id/textCultura"
        android:layout_width="115dp"
        android:layout_height="45dp"

        android:layout_marginTop="40dp"
        android:fontFamily="monospace"
        android:text="Cultura"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="26dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgCultura" />

      <ImageView
        android:id="@+id/imgCultura"
        android:layout_width="160dp"
        android:layout_height="130dp"
        android:layout_marginTop="96dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bandera"
        tools:layout_editor_absoluteY="214dp" />

    <ImageView
        android:id="@+id/img_Log"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logosinnombre" />


    <ImageButton
        android:id="@+id/eventoC4"
        android:layout_width="360dp"
        android:layout_height="130dp"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="825dp" />

    <ImageButton
        android:id="@+id/eventoC5"
        android:layout_width="360dp"
        android:layout_height="130dp"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/eventoC4"
        tools:layout_editor_absoluteX="28dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="323dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="139dp"
        tools:layout_editor_absoluteY="464dp"
        tools:srcCompat="@tools:sample/avatars" />


</androidx.constraintlayout.widget.ConstraintLayout>